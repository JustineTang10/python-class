import turtle

def shapedrawer(turtlename, sidenumber, sidelength=100):
  for i in range(sidenumber):
    turtlename.fd(sidelength)
    turtlename.left(360/sidenumber)

x=turtle.Turtle()
x.shape('turtle')
x.color('lightBlue')
x.shapesize(1,0.5)
for i in range(36):
  shapedrawer(x, 3, 50)
  x.right(10)
